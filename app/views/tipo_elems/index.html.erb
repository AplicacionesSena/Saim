<h1>Listing tipo_elems</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_elems.each do |tipo_elem| %>
      <tr>
        <td><%= tipo_elem.nombre %></td>
        <td><%= tipo_elem.descripcion %></td>
        <td><%= link_to 'Show', tipo_elem %></td>
        <td><%= link_to 'Edit', edit_tipo_elem_path(tipo_elem) %></td>
        <td><%= link_to 'Destroy', tipo_elem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo elem', new_tipo_elem_path %>
