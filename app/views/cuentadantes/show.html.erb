<p id="notice"><%= notice %></p>

<p>
  <strong>Nombre:</strong>
  <%= @cuentadante.nombre %>
</p>

<p>
  <strong>Tipo doc:</strong>
  <%= @cuentadante.tipo_doc.sigla %>
</p>

<p>
  <strong>Documento:</strong>
  <%= @cuentadante.documento %>
</p>

<p>
  <strong>Telefono:</strong>
  <%= @cuentadante.telefono %>
</p>

<%= button_to 'Editar', edit_cuentadante_path(@cuentadante), method: "get", class: 'btn btn-warning' %> |
<%= button_to 'Atras', cuentadantes_path, method: "get", class: 'btn btn-success' %>
